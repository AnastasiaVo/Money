{
  "ConnectionStrings": {
    "Application": "Filename=data/Application.db",
    "EventSourcing": "Filename=data/EventSourcing.db",
    "ReadModel": "Filename=data/ReadModel.db"
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "Identity": {
    "Password": {
      "RequireDigit": false,
      "RequiredLength": 4,
      "RequireLowercase": false,
      "RequireNonAlphanumeric": false,
      "RequireUppercase": false
    }
  }
}
